    "css-loader": "^0.26.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-packager": "^12.1.0",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.8.4",
    "node-libs-browser": "^2.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.7.2"
  },
